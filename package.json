{
  "name": "@briskhome/core",
  "version": "0.3.0",
  "description": "",
  "main": "lib/core.js",
  "scripts": {
    "babel": "node_modules/.bin/babel src -d lib --copy-files --ignore node_modules",
    "babel-watch": "vagrant up && node_modules/.bin/babel src --watch --out-dir lib --copy-files --ignore node_modules & yarn webpack-watch",
    "build": "yarn build-cleanup && yarn babel && yarn webpack",
    "build-cleanup": "rm -rf lib/*",
    "build-watch": "yarn build-cleanup && yarn babel-watch",
    "flow": "node_modules/.bin/flow",
    "lint": "eslint src --ext .js,.jsx --ignore-pattern '**/*.spec.js' --ignore-pattern '**/__mocks__' --ignore-pattern '**/__tests__/*' --ignore-pattern '**/__css__/*' --ignore-pattern '**/__e2e__/*' --ignore-pattern '**/*_spec.js'",
    "start": "node lib/core.js | bunyan -o short",
    "start-watch": "node_modules/.bin/nodemon lib/core.js | node_modules/.bin/bunyan -o short",
    "start-stop": "node lib/core.js --stop",
    "start-ugly": "node lib/core.js --ugly | bunyan -o short",
    "test": "node_modules/.bin/jest --testPathPattern='__tests__'",
    "test-css": "node_modules/.bin/jest --testPathPattern='__css__' --env='./utilities/jest/chromeEnvironment.js'",
    "test-e2e": "node_modules/.bin/jest --testPathPattern='__e2e__' --env='./utilities/jest/chromeEnvironment.js'",
    "postinstall": "npm config set briskhome_dir /opt/briskhome",
    "webpack": "node_modules/.bin/webpack --config ./src/core.webapp/webpack.config.js --context ./src/core.webapp",
    "webpack-watch": "node_modules/.bin/webpack --config ./src/core.webapp/webpack.config.js --context ./src/core.webapp --watch",
    "start-storybook": "start-storybook -c ./src/core.webapp/.storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Egor Zaitsev <ezaitsev@briskhome.com>",
  "license": "MIT",
  "engines": {
    "node": ">=5.0.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.5",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-inline-imports-commonjs": "^1.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.0.0",
    "css-loader": "^0.28.4",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "3.3.4",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "0.75.0",
    "jest": "23.2.0",
    "jest-image-snapshot": "^2.2.1",
    "prettier": "1.13.6",
    "puppeteer": "^1.5.0",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^16.4.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^4.12.1"
  },
  "dependencies": {
    "agenda": "^1.0.3",
    "async": "^2.1.4",
    "bcrypt": "^2.0.1",
    "bunyan": "^1.8.1",
    "connect-mongodb-session": "^2.0.3",
    "cookie-parser": "^1.4.3",
    "dataloader": "^1.3.0",
    "eventemitter2": "^5.0.1",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "express-session": "^1.15.6",
    "graphql": "0.13.2",
    "graphql-tools": "^3.0.4",
    "mongoose": "^5.1.6",
    "nconf": "^0.10.0",
    "node-forge": "^0.7.5",
    "node-telegram-bot-api": "^0.30.0",
    "nodemon": "^1.11.0",
    "passport": "^0.4.0",
    "properties": "1.2.1",
    "rc-menu": "^7.1.0",
    "react": "16.4.1",
    "react-apollo": "2.1.6",
    "react-aria-menubutton": "^6.0.0",
    "react-dom": "16.4.1",
    "react-dropdown": "heuels/react-dropdown#disabled_classname",
    "react-flexbox-grid": "^2.1.2",
    "react-grid-layout": "^0.16.2",
    "react-input-mask": "^2.0.2",
    "react-menu-list": "^5.0.0",
    "react-minimal-dropdown": "^1.1.1",
    "react-modal": "3.4.5",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "react-simple-dropdown": "^3.0.0",
    "react-toggle": "^4.0.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "typeface-roboto": "^0.0.54",
    "useragent": "^2.2.1",
    "uuid-1345": "^0.99.6"
  },
  "jest": {
    "rootDir": "src",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/__css__/*",
      "!**/__e2e__/*",
      "!**/__mocks__/*",
      "!**/__stories__/*",
      "!**/__tests__/*",
      "!core.webapp/webpack.config.js",
      "!utilities/jest/*"
    ],
    "coverageDirectory": "<rootDir>/../coverage",
    "moduleNameMapper": {
      "\\.(css|styl)$": "<rootDir>/core.webapp/__mocks__/styl.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/utilities/jest/index.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
